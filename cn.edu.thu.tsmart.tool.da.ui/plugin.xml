<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.jdt.junit.ResultView">
         <command
               commandId="cn.edu.thu.tsmart.tool.da.ui.traceAllTests"
               icon="icons/Go.png"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.jdt.junit.ResultView">
         <command
               commandId="cn.edu.thu.tsmart.tool.da.ui.cancelResolving"
               icon="icons/pause.png"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:cn.edu.thu.tsmart.tool.da.ui.views.suggestionview">
         <command
               commandId="cn.edu.thu.tsmart.tool.da.ui.applySuggestion"
               icon="icons/apply.png"
               label="Apply"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.jdt.junit.ResultView">
         <command
               commandId="cn.edu.thu.tsmart.tool.da.ui.continueResolving"
               icon="icons/resume.png"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.jdt.junit.ResultView">
         <command
               commandId="cn.edu.thu.tsmart.tool.da.ui.collectCheckpoints"
               icon="icons/c_moe.png"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="cn.edu.thu.tsmart.tool.da.ui.traceAllTests"
            name="Start Resolving">
      </command>
      <command
            id="cn.edu.thu.tsmart.tool.da.ui.cancelResolving"
            name="Cancel Resolving">
      </command>
      <command
            id="cn.edu.thu.tsmart.tool.da.ui.applySuggestion"
            name="Apply Suggestion">
      </command>
      <command
            id="cn.edu.thu.tsmart.tool.da.ui.continueResolving"
            name="Continue Resolving">
      </command>
      <command
            id="cn.edu.thu.tsmart.tool.da.ui.collectCheckpoints"
            name="Collect Checkpoints">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="cn.edu.thu.tsmart.tool.da.ui.handler.StartResolveHandler"
            commandId="cn.edu.thu.tsmart.tool.da.ui.traceAllTests">
      </handler>
      <handler
            class="cn.edu.thu.tsmart.tool.da.ui.handler.CancelResolveHandler"
            commandId="cn.edu.thu.tsmart.tool.da.ui.cancelResolving">
      </handler>
      <handler
            class="cn.edu.thu.tsmart.tool.da.ui.handler.ApplySuggestionHandler"
            commandId="cn.edu.thu.tsmart.tool.da.ui.applySuggestion">
      </handler>
      <handler
            class="cn.edu.thu.tsmart.tool.da.ui.handler.ContinueResolvingHandler"
            commandId="cn.edu.thu.tsmart.tool.da.ui.continueResolving">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="cn.edu.thu.tsmart.tool.da.ui.views.SuggestionView"
            id="cn.edu.thu.tsmart.tool.da.ui.views.suggestionview"
            name="Fix Suggestions"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="cn.edu.thu.tsmart.tool.da.ui.views.TraceAndCheckpointView"
            id="cn.edu.thu.tsmart.tool.da.ui.views.TraceAndCheckpointView"
            name="Execution Trace and Checkpoint"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="cn.edu.thu.tsmart.tool.da.ui.views.FaultLocalizationView"
            id="cn.edu.thu.tsmart.tool.da.ui.faultlocalizationview"
            name="Fault Localization"
            restorable="true">
      </view>
   </extension>


</plugin>
